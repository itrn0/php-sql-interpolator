{
  "name": "itrn0/php-sql-interpolator",
  "type": "library",
  "description": "A library for interpolating variables into SQL strings in a safe and simple way.",
  "authors": [
    {
      "name": "itrn0"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Itrn0\\SqlInterpolator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Itrn0\\Tests\\SqlInterpolator\\": "tests/"
    }
  },
  "scripts": {
    "tests": "./vendor/bin/phpunit"
  }
}
